<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yun.tabswitch" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".TabActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RadioButtonFm"
            android:label="@string/title_activity_radio_button_fm" >
        </activity>
        <activity
            android:name=".RadioButtonFM2"
            android:label="@string/title_activity_radio_button_fm2" >
        </activity>
        <activity
            android:name=".TabHostFM"
            android:label="@string/title_activity_tab_host_fm" >
        </activity>
    </application>

</manifest>
